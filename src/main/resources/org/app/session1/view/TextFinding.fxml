<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="407.0" prefWidth="416.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.app.session1.controller.TextFindingController">
    <children>
        <TextArea fx:id="textAreaOutput" layoutX="35.0" layoutY="176.0" prefHeight="200.0" prefWidth="340.0" />
      <Pane layoutX="22.0" layoutY="23.0" prefHeight="153.0" prefWidth="366.0">
         <children>
            <Button fx:id="chooseTextPositionBtn" layoutX="20.0" layoutY="14.0" mnemonicParsing="false" onAction="#onChooseTextBtn" text="Choose Text Position" />
              <Button fx:id="getTextBtn" layoutX="275.0" layoutY="12.0" mnemonicParsing="false" onAction="#onGetText" text="Get Text" />
              <TextField fx:id="startPositionInput" layoutX="192.0" layoutY="55.0" prefHeight="26.0" prefWidth="160.0" />
              <TextField fx:id="endingPositionInput" layoutX="192.0" layoutY="102.0" />
            <Label layoutX="31.0" layoutY="58.0" text="Starting Position" />
            <Label layoutX="34.0" layoutY="107.0" text="Ending Position" />
         </children>
      </Pane>
    </children>
</AnchorPane>

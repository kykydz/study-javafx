<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="340.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.app.session1.controller.S5ImageManagementController">
   <children>
      <Text fx:id="myText" layoutX="22.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />

      <Button fx:id="uploadButton" layoutX="22.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleUploadButton" text="Upload" />
      <Button fx:id="saveButton" disable="true" layoutX="126.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleSaveButton" text="Save" />
      <Button fx:id="showButton" disable="true" layoutX="228.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleShowButton" text="Show Random" />
      <TitledPane layoutX="22.0" layoutY="95.0" prefHeight="200.0" prefWidth="200.0" text="Image 1">
         <content>
            <ImageView fx:id="imageView1" fitHeight="184.0" fitWidth="212.0" pickOnBounds="true" preserveRatio="true" />
         </content>
      </TitledPane>
      <TitledPane layoutX="304.0" layoutY="95.0" prefHeight="152.0" prefWidth="265.0" text="Image 2 (Stretch Horizontal)">
         <content>
            <ImageView fx:id="imageView2" fitHeight="99.0" fitWidth="227.0" pickOnBounds="true" preserveRatio="true" />
         </content>
      </TitledPane>
   </children>
</AnchorPane>
